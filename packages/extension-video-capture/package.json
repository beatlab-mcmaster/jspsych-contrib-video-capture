    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beatlab-mcmaster/AVOKE/issues"
  },
  "homepage": "https://github.com/beatlab-mcmaster/AVOKE/tree/main/extension-video-capture",
